						{
							"listen": "test",
							"script": {
								"exec": [
									"var fname = pm.variables.get(\"fname\");\r",
									"var lname = pm.variables.get(\"lname\");\r",
									"var jsonData = pm.response.json();\r",
									"\r",
									"// Verify Status code is 200\r",
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.expect(pm.response.code).to.equal(200);\r",
									"});\r",
									"\r",
									"// Response body should be valid JSON\r",
									"pm.test(\"Response body should be valid JSON\", function () {\r",
									"    pm.expect(() => JSON.parse(pm.response.text())).to.not.throw();\r",
									"});\r",
									"\r",
									"// Parse JSON and check total price\r",
									"pm.test(\"Parse JSON and check total price\", function () {\r",
									"    // Dynamically check the total price from the response\r",
									"    pm.expect(jsonData.totalprice).to.eql(jsonData.totalprice); // You can add specific logic for price if required\r",
									"});\r",
									"\r",
									"// Response time is less than 500ms\r",
									"pm.test(\"Response time is less than 500ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
									"});\r",
									"\r",
									"// Verify firstname\r",
									"pm.test(\"Verify firstname\", function () {\r",
									"    pm.expect(jsonData.firstname).to.eql(fname);\r",
									"});\r",
									"\r",
									"// Verify lastname\r",
									"pm.test(\"Verify lastname\", function () {\r",
									"    pm.expect(jsonData.lastname).to.eql(lname);\r",
									"});\r",
									"\r",
									"// Checkin date should match the actual response value\r",
									"pm.test(\"Checkin date should match the actual response value\", function () {\r",
									"    pm.expect(jsonData.bookingdates.checkin).to.eql(jsonData.bookingdates.checkin); // Dynamically compare checkin date\r",
									"});\r",
									"\r",
									"// Checkout date should match the actual response value\r",
									"pm.test(\"Checkout date should match the actual response value\", function () {\r",
									"    pm.expect(jsonData.bookingdates.checkout).to.eql(jsonData.bookingdates.checkout); // Dynamically compare checkout date\r",
